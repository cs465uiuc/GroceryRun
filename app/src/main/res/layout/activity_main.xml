<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.cs465.groceryrun.groceryrun.MainActivity"
    android:background="#FF67C7ED">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Welcome to Grocery Run"
        android:id="@+id/textView13"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton"
        android:text="@string/icon_user"
        android:textSize="24dp"
        android:onClick="profile"
        android:layout_marginTop="21dp"
        android:layout_below="@+id/textView13"
        android:layout_toLeftOf="@+id/textView20"
        android:layout_toStartOf="@+id/textView20" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton2"
        android:text="@string/icon_news"
        android:textSize="24dp"
        android:onClick="request"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/button18"
        android:layout_toLeftOf="@+id/textView19"
        android:layout_toStartOf="@+id/textView19" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton3"
        android:text="@string/icon_usd"
        android:textSize="24dp"
        android:onClick="transactions"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/imageButton2"
        android:layout_alignLeft="@+id/imageButton4"
        android:layout_alignStart="@+id/imageButton4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton4"
        android:text="@string/icon_gear"
        android:textSize="24dp"
        android:onClick="settings"
        android:layout_below="@+id/imageButton3"
        android:layout_toLeftOf="@+id/textView21"
        android:layout_toStartOf="@+id/textView21"
        android:layout_marginTop="22dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton5"
        android:text="@string/icon_phone"
        android:textSize="24dp"
        android:layout_marginTop="21dp"
        android:onClick="contact"
        android:layout_below="@+id/imageButton4"
        android:layout_alignLeft="@+id/imageButton4"
        android:layout_alignStart="@+id/imageButton4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:id="@+id/textView18"
        android:layout_marginRight="50dp"
        android:layout_marginEnd="50dp"
        android:layout_alignBaseline="@+id/imageButton"
        android:layout_alignBottom="@+id/imageButton"
        android:layout_alignRight="@+id/textView13" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Request"
        android:id="@+id/textView19"
        android:layout_alignBaseline="@+id/imageButton2"
        android:layout_alignBottom="@+id/imageButton2"
        android:layout_alignRight="@+id/textView18"
        android:layout_alignEnd="@+id/textView18" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transactions"
        android:id="@+id/textView20"
        android:layout_alignBaseline="@+id/imageButton3"
        android:layout_alignBottom="@+id/imageButton3"
        android:layout_alignLeft="@+id/textView21"
        android:layout_alignStart="@+id/textView21" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Settings"
        android:id="@+id/textView21"
        android:layout_alignBaseline="@+id/imageButton4"
        android:layout_alignBottom="@+id/imageButton4"
        android:layout_alignRight="@+id/textView19"
        android:layout_alignEnd="@+id/textView19" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact Us"
        android:id="@+id/textView22"
        android:layout_alignBaseline="@+id/imageButton5"
        android:layout_alignBottom="@+id/imageButton5"
        android:layout_toRightOf="@+id/imageButton5"
        android:layout_toEndOf="@+id/imageButton5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button18"
        android:text="@string/icon_car"
        android:textSize="24dp"
        android:onClick="deliver"
        android:layout_marginTop="21dp"
        android:layout_below="@+id/imageButton"
        android:layout_toLeftOf="@+id/textView23"
        android:layout_toStartOf="@+id/textView23" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deliver"
        android:id="@+id/textView23"
        android:layout_alignBaseline="@+id/button18"
        android:layout_alignBottom="@+id/button18"
        android:layout_alignLeft="@+id/textView19"
        android:layout_alignStart="@+id/textView19" />

</RelativeLayout>
